{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:container": "CONTAINER_MODE=true vite --host 0.0.0.0 --port 3000",
    "build": "tsc -b && vite build",
    "build:verify": "npm run type-check && npm run lint && npm run build",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "clean": "rm -rf dist node_modules/.vite",
    "docker:build": "docker build -f ../Dockerfile.frontend -t fin-frontend .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.development fin-frontend",
    "workflow": "./dev-workflow.sh",
    "workflow:backend": "./dev-workflow.sh backend",
    "start:containers": "../start.sh"
  },
  "dependencies": {
    "@paypal/paypal-js": "^9.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "lucide-react": "^0.553.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.0.1",
    "vitest": "^1.3.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-arm64-gnu": "^4.53.3"
  }
}
